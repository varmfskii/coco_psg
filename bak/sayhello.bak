	org $7000
start:
	;; initialize variables
	ldx #$ff00
	ldy #$ff7e
	;; reset ssc
	lda #1
	sta -1,y
	clr -1,y
	;; set coco sound multiplexer to cartridge input
	lda #52
	sta 1,x
	lda #63
	sta 3,x
	;; enable sound multiplexer
	lda #60
	sta 35,x
	;; string loop
	ldu #string
loop:
	bsr wait
	lda ,u+
	sta ,y
	cmpa #13
	bne loop
	rts
wait:	
	lda ,y
	anda #$80
	beq wait
	rts
string:
	fcc "HELLO COLOR COMPUTER "
	fcb 13
	end start
	
